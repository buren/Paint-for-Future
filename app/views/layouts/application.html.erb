<body>
  <%= render :partial => "home/header" %>
      
     <% flash.each do |name, msg| %>
        <%= content_tag :div, raw(msg), :id => "flash_#{name}" %>
      <% end %>
  <%= yield %>

  <%= render :partial => "home/footer" %>

</body>
</html>
