<title> Paint for Future - Partners</title><div id="wrapper">  		<ul class="videos">		<li><h1>Sponsors and Partners</h1></li>		<% @videos.each do |video| %>	  		<li><h1><%= video.title if video.title? %></h1></li>	    	<li>	    		<% if video.logo? && video.homepage? %>		    		<%= link_to video.homepage, :class => "external-links" do %>			    		<%= image_tag video.logo %> 		    		<% end %>	    		<% end %>	    		<br>	    		<%= video.description.html_safe if video.description? %>	    		<br>	    		<%= link_to video.homepage, video.homepage, :class => "external-links" %> - <%= video.homepage_slogan %>	    	</li>	    	<br>	    	<br>		<% end %>	</ul>  <div style="margin-bottom:30px;">    <%= render :partial => "home/paypal_donate" %>  </div></div>